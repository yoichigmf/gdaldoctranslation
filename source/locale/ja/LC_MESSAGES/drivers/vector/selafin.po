#: ../../source/drivers/vector/selafin.rst:71
msgid "string,"
msgstr ""

#: ../../source/drivers/vector/selafin.rst:72
msgid "floating point values,"
msgstr ""

#: ../../source/drivers/vector/selafin.rst:73
